{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Products": {
    "MaximumCount": 10
  },
  "FileCache": {
    "Dir": "cache",
    "MaxCachedCount": 10,
    "ExpirationTime": "01:00:00"
  },
  "LoggingFilter": {
    "LogParameters": true
  }
}
